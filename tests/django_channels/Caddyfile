{
	http_port 9080
	https_port 9443
	log {
		level info
	}
}
localhost:9080 {
	route /ws/* {
		python {
			module_asgi "mysite.asgi:application"
			workers 1
			venv "./venv"
		}
	}

	route /item/* {
		python {
			module_asgi "mysite.asgi:application"
			workers 1
			venv "./venv"
		}
	}

	route / {
		respond 404
	}
}
